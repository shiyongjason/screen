}
.fl-item{
  width:630px;
  height:350px;
  margin: 0 auto;
  /*margin-left: 4px;*/
  /*margin-right: 8px;*/

}
.fl-item table{
  /*border:solid 1px;*/
  margin: 0;
  padding: 0;
  width:100%;
}
.fl-item table tr:nth-child(1){
  height:250px;
}
.fl-item table tr:nth-child(1) td{
  border-bottom: 0;
}
.fl-item table tr:nth-child(2){
  height: 95px;
}
.fl-item table tr:nth-child(2) td{
  width:33.33%;
}
.fl-item table tr:nth-child(2) td span{
  display: block;
}
.fl-item table tr:nth-child(2) td:nth-child(2){
  border-left: 0;
  border-right: 0;
}


.fl-item table tr td{
  border:solid 2px #2b2e6b;
  position: relative;
}
.fl-item>ul{
  list-style:none; /* 去掉ul前面的符号 */
  margin: 0px; /* 与外界元素的距离为0 */
  padding: 0px; /* 与内部元素的距离为0 */
}
.fl-item>ul>li{
  display: inline-block;
  border: solid 2px #2b2e6b;
  margin: 1px;
  width:30%
}
